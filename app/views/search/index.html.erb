<div id="results">
  <h3>Displaying <%= @stores.count %> Stores</h3>

  <table class="table">
    <th>Long Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone Number</th>
    <th>Store Type</th>
    <% @stores.each_with_index do |store, index| %>
      <tr class="results" id="result-<%= index + 1 %>">
        <td><%= link_to(store.longName, search_path(store.storeId))%></td>
        <td><%= store.city %></td>
        <td><%= store.distance %></td>
        <td><%= store.phone %></td>
        <td><%= store.type %></td>
      </tr>
    <% end %>
  </table>
</div>
